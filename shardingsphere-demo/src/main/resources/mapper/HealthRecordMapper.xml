<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.apache.shardingsphere.shardingspheredemo.repository.HealthRecordRepository">
    <insert id="addEntity" parameterType="org.apache.shardingsphere.shardingspheredemo.entity.HealthRecord"
            useGeneratedKeys="true" keyProperty="recordId">
        INSERT INTO health_record(user_id, level_id, remark)
        VALUES (#{userId}, #{levelId}, #{remark})
    </insert>
</mapper>